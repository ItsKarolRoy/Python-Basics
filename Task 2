import numpy as np
import matplotlib.pyplot as plt
from scipy import stats

data2.columns

data2.drop(['Observation'] , axis=1, inplace=True)

data2.columns

Q1 = data2.quantile(0.25) # Corrected function name to 'quantile'
Q3 = data2.quantile(0.75) # Corrected function name to 'quantile'
IQR = Q3 - Q1
print(IQR)

data2=data2[~((data2<(Q1-1.5*IQR))|(data2>(Q3+1.5*IQR))).any(axis=1)]
data2

data2.describe()
